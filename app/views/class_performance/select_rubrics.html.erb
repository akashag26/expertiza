<h1> Class Performance Rubric</h1>
<h4> Select any number of rubric criterias from the following </h4>

<center>
<select multiple size = 10>
<% @results.each do |result| %>
  <option value="value">  <%= result.txt %> </option>
<% end %>
</select>
</center>

<%= form_for :class_performance do |f| %>
  <%= f.select :phone_type, options_for_select([["Select Rubric Criterias", ""],  @results ]), :prompt => 'Select One' %>
  <%= select_tag "select criterias", options_for_select(@results.map { |obj| [obj['txt'], obj['id']] }) %>

<% end %>
